{
  "name": "joro/typo3reversedeployment",
  "description": "Export DB and files to your TYPO3 dev installation",
  "license": "GPL-3.0-or-later",
  "require": {
    "php": "^7.2 || ^8.0",
    "symfony/process": "^5.3"
  },
  "autoload": {
    "psr-4": {
      "JoRo\\": "JoRo/"
    }
  },
  "bin": [
    "typo3reverse"
  ],
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^0.12.99"
  },
  "extra": {
    "phar-builder": {
      "compression": "GZip",
      "name": "typo3reverse.phar",
      "output-dir": "./",
      "entry-point": "./typo3reverse",
      "include": ["JoRo"],
      "include-dev": false
    }
  }
}
